<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.rap.ui.entrypoint">
      <entrypoint
            brandingId="org.polymap.p4.branding"
            class="org.polymap.rhei.batik.BatikApplication"
            id="org.polymap.p4.entrypoint"
            path="/p4">
      </entrypoint>
   </extension>
   <extension
         point="org.eclipse.rap.ui.branding">
      <branding
            id="org.polymap.p4.branding"
            themeId="org.polymap.rhei.batik.materialDesign"
            title="P4">
         <additionalHeaders>
            <link
                  href="batikres/css/roboto.css"
                  rel="stylesheet">
            </link>
         </additionalHeaders>
      </branding>
   </extension>
   <extension
         point="org.polymap.rhei.batik.design">
      <design
            class="org.polymap.p4.P4AppDesign"
            id="org.polymap.p4.design"
            servletNameMatcher="/p4">
      </design>
   </extension>
   <extension
         point="org.polymap.rhei.batik.panels">
      <panel
            class="org.polymap.p4.map.ProjectMapPanel"
            id="org.polymap.batikapp.ProjectPanel">
      </panel>
      <panel
            class="org.polymap.p4.project.LayersPanel"
            id="org.polymap.batikapp.LayersPanel"
            stackPriority="100">
      </panel>
      <panel
            class="org.polymap.p4.imports.ShapeImportPanel"
            id="org.polymap.p4.ImportPanel">
      </panel>
      <panel
            class="org.polymap.p4.catalog.CatalogPanel"
            id="org.polymap.p4.CatalogPanel">
      </panel>
      <panel
            class="org.polymap.p4.catalog.ResourceInfoPanel"
            id="org.polymap.p4.ResourceInfoPanel">
      </panel>
      <panel
            class="org.polymap.p4.project.LayerInfoPanel"
            id="org.polymap.p4.project.LayerPanel">
      </panel>
      <panel
            class="org.polymap.p4.project.ProjectInfoPanel"
            id="org.polymap.p4.project.ProjectInfoPanel">
      </panel>
      <panel
            class="org.polymap.p4.data.imports.ImportPanel"
            id="org.polymap.p4.ImportPanel">
      </panel>
      <panel
            class="org.polymap.p4.style.StylerPanel"
            id="org.polymap.p4.StylerPanel">
      </panel>
      <panel
            class="org.polymap.p4.style.color.ColorPanel"
            id="org.polymap.p4.ColorPanel">
      </panel>
      <panel
            class="org.polymap.p4.style.icon.ImagePanel"
            id="org.polymap.p4.ImagePanel">
      </panel>
      <panel
            class="org.polymap.p4.style.font.FontPanel"
            id="org.polymap.p4.FontPanel">
      </panel>
   </extension>
   <extension
         point="org.polymap.service.fs.contentProviders">
      <contentProvider
            class="org.polymap.p4.fs.ProjectContentProvider"
            id="org.polymap.p4.fs.ProjectContentProvider"
            name="Projects">
      </contentProvider>
      <contentProvider
            class="org.polymap.p4.fs.ShapefileContentProvider"
            id="org.polymap.p4.fs.ShapefileContentProvider"
            name="Shapefile">
      </contentProvider>
      <contentProvider
            class="org.polymap.p4.fs.GeoJsonContentProvider"
            id="org.polymap.p4.fs.GeoJsonContentProvider"
            name="GeoJSON">
      </contentProvider>
      <contentProvider
            class="org.polymap.p4.fs.SldContentProvider"
            id="org.polymap.p4.fs.SldContentProvider"
            name="SLD">
      </contentProvider>
      <contentProvider
            class="org.polymap.p4.fs.CsvContentProvider"
            id="org.polymap.p4.fs.CsvContentProvider"
            name="CSV">
      </contentProvider>
      <contentProvider
            class="org.polymap.p4.fs.WorkbenchContentProvider"
            id="org.polymap.p4.fs.WorkbenchContentProvider"
            name="Workbench">
      </contentProvider>
   </extension>

</plugin>
